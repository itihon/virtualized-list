{
  "name": "@itihon/virtualized-list",
  "version": "0.0.1",
  "description": "A virtualized list powered by AVL-tree and based on Web Components API",
  "homepage": "https://itihon.github.io/virtualized-list/",
  "keywords": [
    "virtualized",
    "virtualized-list",
    "window",
    "windowing"
  ],
  "author": "Alexandr Kalabin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itihon/virtualized-list.git"
  },
  "bugs": {
    "url": "https://github.com/itihon/virtualized-list/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:bundle && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "vite build",
    "build:demo": "vite build -c vite.demo.config.js -m DEMO_BUILD",
    "dev:demo": "vite -c vite.demo.config.js -m DEMO_DEV",
    "preview": "vite preview",
    "format:check": "prettier {src,test}/**/*.{js,ts,css} --check --no-error-on-unmatched-pattern",
    "format:write": "prettier {src,test}/**/*.{js,ts,css} --write --no-error-on-unmatched-pattern",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/prismjs": "^1.26.5",
    "@types/wicg-file-system-access": "^2023.10.6",
    "cypress": "^14.5.0",
    "cypress-vite": "^1.6.0",
    "eslint": "^10.0.0",
    "eslint-plugin-headers": "^1.3.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.1",
    "prismjs": "^1.30.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  },
  "lint-staged": {
    "{src,test}/**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "overrides": {
    "vite": "^7.0.0"
  },
  "dependencies": {
    "avl": "^2.0.0",
    "request-animation-frame-loop": "^1.0.2"
  }
}
